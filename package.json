{
  "name": "design.samking.co",
  "version": "1.0.0",
  "description": "Temporary design portfolio",
  "scripts": {
    "clean:api": "rm -rf build/api/*",
    "prebuild:api": "yarn run clean:api",
    "prebuild": "yarn run lint && yarn run test",
    "build": "yarn run build:www && yarn run build:api",
    "build:www": "NODE_ENV=production webpack -p --progress",
    "build:api": "VIPS_WARNING=false node --harmony-async-await src/api",
    "start": "webpack-dev-server",
    "lint": "eslint src/**/*.js",
    "test": "jest"
  },
  "main": "index.js",
  "repository": "github.com/samisking/design.samking.co",
  "author": "Sam King",
  "license": "MIT",
  "dependencies": {
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "raf-throttle": "^1.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "next",
    "react-router-dom": "next",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sk-fetch-wrapper": "^0.0.7"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-react-app": "^0.5.2",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "loader-utils": "0.2.16",
    "mkdirp": "^0.5.1",
    "postcss-for": "^2.1.1",
    "postcss-loader": "^1.3.0",
    "postcss-modules-values": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.4.2",
    "react-transform-hmr": "^1.0.4",
    "sharp": "^0.17.2",
    "style-loader": "^0.13.1",
    "url": "^0.11.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy"
    }
